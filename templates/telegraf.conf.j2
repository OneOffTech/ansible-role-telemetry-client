# Global tags can be specified here in key="value" format.
[global_tags]

# Configuration for telegraf agent
[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = "" # same magnitude as interval
  debug=false
  quiet=false
  logfile = "" # log to stdout
  hostname = "" # use os.Hostname()
  omit_hostname = false

[[outputs.influxdb]]
  urls = ["{{ telegraf_url }}"]
  database = "telegraf"
  username = "{{ telegraf_user }}"
  password = "{{ telegraf_password }}"
  retention_policy = ""
  write_consistency = "any"
  timeout = "5s"

[[inputs.cpu]]
  percpu = false
  totalcpu = true
  collect_cpu_time = false
  report_active = false

[[inputs.disk]]
  ignore_fs = ["tmpfs", "devtmpfs", "devfs"]

[[inputs.diskio]]

[[inputs.mem]]

[[inputs.swap]]

[[inputs.system]]

[[inputs.net]]

[[inputs.sensors]]

[[inputs.smart]]
